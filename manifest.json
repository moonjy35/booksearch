{
  "name": "쉬운 도서 검색",
  "version": "0.1",
  "description": "쉬운 도서 검색",
  "icons": { 
    "16": "icon.png",
    "48": "icon_48.png",
    "128": "icon_144.png"
  },
  "permissions": ["contextMenus", "tabs", "<all_urls>"],
  "background": {
    "page" : "background.html",
    //"scripts": ["background.js"]
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "background.html"
  },
  "manifest_version": 2
}